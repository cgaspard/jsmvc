{
    "mvcRoot": "/",
    "autoInit": true,
    "listenForHashChanges": true,
    "loadHashOnInit": true,
    "screenContainerSelector": "#mvc-screens",
    "cssContainerSelector": "#mvc-csscontainer",
    "templateSelector": ".mvc-template",
    "views": ["/mvc/home.view.js", "/mvc/today.view.js", "/mvc/handle.view.js"],
    "models": ["/mvc/home.model.js", "/mvc/today.model.js", "/mvc/handle.model.js"],
    "routes": [{
        "pattern": "#home/:id/test/:name",
        "view": "home",
        "model": "home"
    }, {
        "pattern": "#today",
        "view": "today",
        "model": "today"
    }, {
        "pattern": "#handle",
        "view": "handle",
        "model": "handle"
    }]
}
